@font-face
  font-family 'icomoon'
  src:url('../fonts/icomoon.eot?nuhmxp')
  src:url('../fonts/icomoon.eot?nuhmxp#iefix') format('embedded-opentype'),
    url('../fonts/icomoon.ttf?nuhmxp') format('truetype'),
    url('../fonts/icomoon.woff?nuhmxp') format('woff'),
    url('../fonts/icomoon.svg?nuhmxp#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-icon-facebook
  &:before
    content "\ea90"

.icon-icon-twitter
  &:before
    content "\e902"

.icon-icon-instagram
  &:before
    content "\ea92"

.icon-icon-directions_run
    &:before
      content "\e900"
.icon-icon-home
    &:before
      content "\e901"
.icon-icon-old-phone
    &:before
      content "\e903"
.icon-file:before {
  content: "\e926";
}
.icon-search:before {
  content: "\e986";
}

.icon-icon-user:before {
  content: "\e904";
}
